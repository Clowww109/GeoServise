{
    "swagger": "2.0",
    "info": {
        "description": "Api server for GeoServise",
        "title": "GeoService",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/",
    "paths": {
        "/api/address/geocode": {
            "post": {
                "description": "return closest address to point",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "geocode"
                ],
                "summary": "getGeocode",
                "parameters": [
                    {
                        "description": "Нужно указать query(улица + дом в строке)",
                        "name": "input",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/services.RequestAddressGeocode"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK"
                    },
                    "400": {
                        "description": "Неверный формат запроса"
                    },
                    "500": {
                        "description": "Cервис https://dadata.ru не доступен"
                    }
                }
            }
        },
        "/api/address/search": {
            "post": {
                "description": "return address from geo",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "address"
                ],
                "summary": "getAddress",
                "parameters": [
                    {
                        "description": "Нужно указать query(улица + дом в строке)",
                        "name": "input",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/services.RequestAddressSearch"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK"
                    },
                    "400": {
                        "description": "Неверный формат запроса"
                    },
                    "500": {
                        "description": "Cервис https://dadata.ru не доступен"
                    }
                }
            }
        }
    },
    "definitions": {
        "services.RequestAddressGeocode": {
            "type": "object",
            "properties": {
                "lat": {
                    "type": "string"
                },
                "lng": {
                    "type": "string"
                }
            }
        },
        "services.RequestAddressSearch": {
            "type": "object",
            "properties": {
                "query": {
                    "type": "string"
                }
            }
        }
    }
}